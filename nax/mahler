fl|grep Sym|xargs -d\\n find -L|grep \\.mp4$|sed s_^./__|while read i; do echo "::$i"; f="${i#*/}"; f=${f%.mp4}; [ -e "${i%/*}/.$f.cpl" ] && (if grep -q '[^", -]\+[^"]*\(Classical \)\?Composer' "${i%/*}/.$f.cpl"; then sed 's/^.*\("\([^", -]\+\)[^"]*\(Classical \)\?Composer\).*$/\2\n/' "${i%/*}/.$f.cpl"; else echo; fi; fish=$(sed 's/.*trackno="\([^"]*\)".*shorttracktitle="\([^"]*\)".*trackgroupname="\([^:"(]*\)[:"(].*"\([^"]*\)" persontypeid="12".*\("\([^,]*\)[^"]*\)" persontypeid="9".*/"\3 (\6)\n"\1\n"\2\n\5; \4\n/' "${i%/*}/.$f.cpl"); if [ "${fish:0:5}" = "<?xml" ]; then echo ABORT; else echo "$fish"; fi ); done|recode xml..utf8|sed 's/^"//;s/[; ]*$//;s/  \+/ /g'|sed s/::.*/::/|less

fl|grep Lied|xargs -d\\n find -L|grep \\.mp4$|sed s_^./__|while read i; do echo "::$i"; f="${i#*/}"; f=${f%.mp4}; [ -e "${i%/*}/.$f.cpl" ] && (if grep -q '[^", -]\+[^"]*\(Classical \)\?Composer' "${i%/*}/.$f.cpl"; then sed 's/^.*\("\([^", -]\+\)[^"]*\(Classical \)\?Composer\).*$/\2\n/' "${i%/*}/.$f.cpl"; else echo; fi; fish=$(sed 's/.*trackno="\([^"]*\)".*shorttracktitle="\([^"]*\)".*trackgroupname="\([^:"(]*\)[:"(].*"\([^"]*\)" persontypeid="12".*\("\([^,]*\)[^"]*\)" persontypeid="9".*/"\3 (\6)\n"\1\n"\2\n\5; \4\n/' "${i%/*}/.$f.cpl"); if [ "${fish:0:5}" = "<?xml" ]; then echo ABORT; else echo "$fish"; fi ); done|recode xml..utf8|sed 's/^"//;s/[; ]*$//;s/  \+/ /g'|cpld_straight.exe

fl|grep Sym|xargs -d\\n find -L|grep \\.mp4$|sed s_^./__|while read i; do echo -n ::; printenv i; f="${i#*/}"; f=${f%.mp4}; [ -e "${i%/*}/.$f.cpl" ] && (if grep -q '[^", -]\+[^"]*\(Classical \)\?Composer' "${i%/*}/.$f.cpl"; then sed 's/^.*\("\([^", -]\+\)[^"]*\(Classical \)\?Composer\).*$/\2\n/' "${i%/*}/.$f.cpl"; else echo; fi; fish=$(sed 's/.*trackno="\([^"]*\)".*shorttracktitle="\([^"]*\)".*trackgroupname="\([^:"(]*\)[:"(].*"\([^"]*\)" persontypeid="12".*\("\([^,]*\)[^"]*\)" persontypeid="9".*/"\3 (\6)\n"\1\n"\2\n\5; \4\n/' "${i%/*}/.$f.cpl"); if [ "${fish:0:5}" = "<?xml" ]; then echo ABORT; else echo "$fish"; fi )|recode xml..utf8|sed 's/^"//;s/[; ]*$//;s/  \+/ /g'; done|cpld_straight.exe
